      "import matplotlib.pyplot as plt\n",
      "%pylab inline \n",
      "t = np.arange(0,2,.01)\n",
      "s = 2*(t)+1\n",
      "r = 2*(t)+2\n",
      "v = 0*t+3\n",
      "plt.plot(t,s,t,r,t,v)\n",
      "\n",
      "plt.show()"
     ],
